<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5B272424">

    <TextView
        android:id="@+id/TVabout"
        android:layout_width="355dp"
        android:layout_height="52dp"
        android:layout_marginTop="1dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:gravity="center_vertical"
        android:text="   About Us"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/TVimprove" />

    <TextView
        android:id="@+id/TVimprove"
        android:layout_width="355dp"
        android:layout_height="52dp"
        android:layout_marginTop="1dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:gravity="center_vertical"
        android:text="   Help Us Improve"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/TVrate" />

    <TextView
        android:id="@+id/TVhelp"
        android:layout_width="357dp"
        android:layout_height="52dp"
        android:layout_marginTop="1dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:gravity="center_vertical"
        android:text="   Help Centre / FAQs"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/TVprovide_service" />

    <TextView
        android:id="@+id/TVprovide_service"
        android:layout_width="357dp"
        android:layout_height="52dp"
        android:layout_marginTop="5dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:gravity="center_vertical"
        android:text="   Provide Your Services"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/cardView" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="0dp"
        android:layout_height="95dp"
        android:background="#2CFFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/IVprofile_edit"
            android:layout_width="49dp"
            android:layout_height="41dp"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="15dp"
            android:clickable="true"
            android:contentDescription="TODO"
            app:srcCompat="@drawable/ic_mode_edit_black_24dp" />

        <TextView
            android:id="@+id/TVprofile_pe_name"
            android:layout_width="329dp"
            android:layout_height="35dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="16dp"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/TVprofile_pe_phone"
            android:layout_width="326dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="55dp"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="18sp"
            android:textStyle="bold" />

    </androidx.cardview.widget.CardView>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="59dp"
        android:layout_height="320dp"
        android:layout_marginTop="5dp"
        android:background="#FFFFFF"
        android:clickable="false"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView">

        <ImageView
            android:id="@+id/IVprovide_service"
            android:layout_width="40dp"
            android:layout_height="53dp"
            android:layout_gravity="center_horizontal"
            android:background="#FFFFFF"
            app:srcCompat="@drawable/ic_room_service_black_24dp" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="40dp"
            android:layout_height="53dp"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@drawable/ic_live_help_black_24dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="40dp"
            android:layout_height="53dp"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@drawable/ic_payment_black_24dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="40dp"
            android:layout_height="53dp"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@drawable/ic_star_black_24dp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="40dp"
            android:layout_height="53dp"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@drawable/ic_feedback_black_24dp" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="40dp"
            android:layout_height="53dp"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@drawable/ic_info_black_24dp" />

    </LinearLayout>

    <TextView
        android:id="@+id/TVpayment_options"
        android:layout_width="357dp"
        android:layout_height="52dp"
        android:layout_marginTop="1dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:gravity="center_vertical"
        android:text="   Payment Options"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/TVhelp" />

    <TextView
        android:id="@+id/TVrate"
        android:layout_width="357dp"
        android:layout_height="52dp"
        android:layout_marginTop="1dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:gravity="center_vertical"
        android:text="   Rate Locale Lite"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/TVpayment_options" />

    <TextView
        android:id="@+id/TVlogout"
        android:layout_width="354dp"
        android:layout_height="147dp"
        android:layout_marginTop="1dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:gravity="center_vertical"
        android:text="                               Logout"
        android:textColor="#FF0000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TVabout" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="415dp"
        android:layout_height="68dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TVlogout"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="59dp"
        android:layout_height="147dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/TVlogout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />
</androidx.constraintlayout.widget.ConstraintLayout>